{
  "name": "gatsby_storybook_typescript",
  "version": "1.0.0",
  "description": "A gatsby starter with storybook and typescript",
  "keywords": [
    "gatsby",
    "gatsby-starter",
    "storybook"
  ],
  "homepage": "https://github.com/RobertoMSousa/gatsby-typescript-storybook-starter",
  "license": "MIT",
  "author": {
    "name": "Roberto Sousa",
    "url": "http://rsousa.dev"
  },
  "files": [],
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "https://github.com/RobertoMSousa/gatsby-typescript-storybook-starter"
  },
  "scripts": {
    "develop": "gatsby develop",
    "start": "concurrently --kill-others \"npm run develop\" \"npm run storybook\"",
    "serve": "gatsby serve",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "clean": "gatsby clean",
    "build": "npm run build:gatsby && npm run build:storybook",
    "build:gatsby": "gatsby build",
    "build:storybook": "build-storybook -c .storybook -o public/docs",
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx",
    "format": "prettier --write src/**/*.{js,jsx}",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.0.20",
    "@mdx-js/react": "^1.0.20",
    "babel-plugin-styled-components": "^1.10.6",
    "gatsby": "^2.15.15",
    "gatsby-image": "^2.2.19",
    "gatsby-mdx": "^0.6.3",
    "gatsby-plugin-manifest": "^2.2.16",
    "gatsby-plugin-offline": "^3.0.6",
    "gatsby-plugin-react-helmet": "^3.1.7",
    "gatsby-plugin-sharp": "^2.2.22",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-plugin-typescript": "^2.1.7",
    "gatsby-source-filesystem": "^2.1.22",
    "gatsby-transformer-sharp": "^2.2.14",
    "polished": "^3.4.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1",
    "styled-components": "^4.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@storybook/addon-a11y": "^5.2.5",
    "@storybook/addon-actions": "5.0.11",
    "@storybook/addon-info": "^5.2.5",
    "@storybook/addon-options": "5.0.11",
    "@storybook/addon-viewport": "5.0.11",
    "@storybook/react": "^5.2.5",
    "@types/react-helmet": "^5.0.13",
    "@types/storybook__addon-info": "^4.1.2",
    "@types/storybook__react": "^4.0.2",
    "@types/styled-components": "^4.1.19",
    "@typescript-eslint/eslint-plugin": "^2.2.0",
    "@typescript-eslint/parser": "^2.2.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-preset-react-app": "^9.1.0",
    "eslint": "^6.4.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-compat": "^3.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prefer-object-spread": "^1.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "prettier": "^1.18.2",
    "react-markdown": "^4.0.8",
    "typescript": "^3.6.3"
  }
}
